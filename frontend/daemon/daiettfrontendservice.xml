<service>
	<id>daiettfrontendservice.exe</id>
	<name>DaiettFrontendService</name>
	<description>Daiett App Frontend (static files via Express)</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>F:\Daiett_App\frontend\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>F:\Daiett_App\frontend\frontend-server.cjs</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>DaiettFrontendService wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<logpath>F:\Daiett_App\frontend\logs</logpath>
	<stoptimeout>30sec</stoptimeout>
	<env name="NODE_ENV" value="production"/>
	<serviceaccount>
		<domain>PONCHI</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>F:\Daiett_App\frontend</workingdirectory>
</service>